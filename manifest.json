{
  "manifest_version": 3,
  "name": "SchedulrX",
  "version": "0.1",
  "description": "Reads Gmail emails and adds detected meetings to Google Calendar.",
  "permissions": ["identity", "storage", "scripting"],
  "host_permissions": [
    "https://www.googleapis.com/*"
  ],
  "oauth2": {
    "client_id": "327037733803-dgj2ijnhmph7aikuo5ru3rlk64mngjtc.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/gmail.readonly",
      "https://www.googleapis.com/auth/calendar"
    ]
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/schedulr.png"
  },
  "icons": {
    "128": "icons/schedulr.png"
  }
}
